{
    "_version": "1.49.0",
    "sap.app": {
        "id": "saleorder2",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:ovp",
            "version": "1.10.1",
            "toolsId": "0110b4ee-517c-439f-872f-b721cb4f0947"
        },
        "dataSources": {
            "mainService": {
                "uri": "/s4hanacloud/sap/opu/odata/sap/API_SALES_ORDER_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "annotation"
                    ],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "annotation": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotation.xml",
                "settings": {
                    "localUri": "annotations/annotation.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.115.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.f": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.suite.ui.generic.template": {},
                "sap.ovp": {},
                "sap.ui.rta": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "saleorder2.i18n.i18n"
                }
            },
            "mainModel": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "config": {},
            "routes": [],
            "targets": {}
        }
    },
    "sap.ovp": {
        "globalFilterModel": "mainModel",
        "globalFilterEntityType": "A_SalesOrderType",
        "containerLayout": "resizable",
        "enableLiveFilter": true,
        "considerAnalyticalParameters": false,
        "cards": {
            "list1": {
                "model": "mainModel",
                "template": "sap.ovp.cards.table",
                "settings": {
                    "title": "yassss",
                    "entitySet": "A_SalesOrder",
                    "tabs": [
                        {
                            "annotationPath": "com.sap.vocabularies.UI.v1.LineItem#point2",
                            "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#point1"
                        }
                    ]
                }
            },
            "orderitemcard": {
                "model": "mainModel",
                "template": "sap.ovp.cards.list",
                "settings": {
                    "title": "REPLACE_WITH_TEXT",
                    "entitySet": "A_SalesOrderItem",
                    "listType": "condensed",
                    "listFlavor": "standard",
                    "tabs": [
                        {
                            "annotationPath": "com.sap.vocabularies.UI.v1.LineItem#point4",
                            "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#point3"
                        }
                    ]
                }
            },
            "chartsales": {
                "model": "mainModel",
                "template": "sap.ovp.cards.charts.analytical",
                "settings": {
                    "title": "Sales order",
                    "entitySet": "A_SalesOrder",
                    "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#point5",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#chart1",
                    "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification#point7"
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "analytical"
    }
}